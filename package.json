{
  "name": "ringa",
  "version": "0.0.14",
  "description": "Ringa is a highly extensible ES6 MVC-inspired library designed to help you build scalable and highly debuggable applications.",
  "main": "./dist/ringa.min.js",
  "directories": {
    "example": "example",
    "src": "src"
  },
  "scripts": {
    "test": "jest --config jest.json",
    "build": "npm run dist && npm run dev",
    "dist": "webpack --progress --colors --config build/webpack.dist.js",
    "dev": "webpack --progress --colors --config build/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jung-digital/ringa.git"
  },
  "files": [
    "dist",
    "src",
    "examples"
  ],
  "keywords": [
    "react",
    "framework",
    "mvc",
    "async",
    "chaining",
    "promise",
    "data",
    "state",
    "ioc",
    "redux",
    "flux"
  ],
  "authors": [
    "Joshua Jung <joshua.p.jung@gmail.com> (https://github.com/joshjung)",
    "Thomas Yarnall <corragon@gmail.com> (https://github.com/corragon)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jung-digital/ringa/issues"
  },
  "homepage": "https://github.com/jung-digital/ringa#readme",
  "npmName": "ringa",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "camelcase": "^4.0.0",
    "dateformat": "^2.0.0",
    "error-stack-parser": "^2.0.0",
    "hasharray": "^1.0.0",
    "snake-case": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "webpack": "^2.2.0"
  }
}
